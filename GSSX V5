-- Function to animate the loading bar
function animateLoadingBar()
    local progress = 0

    while progress < 100 do
        progress = progress + 1
        updateProgress(progress)
        wait(0.05) -- Adjust the animation speed as needed
    end

    loadingText.Text = "Successfully Loaded"
  wait(0.5)

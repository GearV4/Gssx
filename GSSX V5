-- Function to change player's speed
local function changeSpeed()
    local enteredSpeed = tonumber(speedInput.Text)
    if enteredSpeed then
        player.Character:WaitForChild("Humanoid").WalkSpeed = enteredSpeed
        print("Speed Set to: " .. enteredSpeed)
    else
        print("Invalid speed input.")
    end
end

-- Function to activate speed script
local function activateSpeedScript()
    -- You can modify this function to perform other actions when the button is clicked
    changeSpeed()
    print("Speed Script Activated!")
end

-- Connect button click to change speed function
continueButton.MouseButton1Click:Connect(changeSpeed)

-- Connect button click to activate speed script function
activateButton.MouseButton1Click:Connect(activateSpeedScript)

-- Function to toggle the GUI visibility
local function toggleGUI()
    frame.Visible = not frame.Visible
end

-- Create Toggle Button
local toggleButton = Instance.new("TextButton")
toggleButton.Text = "Toggle GUI"
toggleButton.Size = UDim2.new(0, 100, 0, 30)
toggleButton.Position = UDim2.new(0, 50, 1, -30)
toggleButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Black button background
toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text color
toggleButton.Font = Enum.Font.SourceSans
toggleButton.TextSize = 14
toggleButton.Parent = gui

-- Connect button click to toggle GUI visibility
toggleButton.MouseButton1Click:Connect(toggleGUI)

-- Delayed function to make the GUI visible after 5 seconds
wait(5)
frame.Visible = true

-- Function to toggle
    print("Clicked")
end)

